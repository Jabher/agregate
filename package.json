{
  "name": "active-graph-record",
  "version": "0.6.1",
  "description": "Schema-less ActiveRecord over neo4j",
  "main": "build/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "build": "./node_modules/.bin/babel lib --out-dir build",
    "preversion": "git add lib/* test/* && npm test && npm run build",
    "postversion": "./node_modules/.bin/jscs --fix lib && git commit --amend --no-edit && git push -u origin master && npm publish && rm -rf ./build"
  },
  "keywords": [
    "activeRecord",
    "db",
    "neo4j",
    "cypher"
  ],
  "author": "Vsevolod Rodionov <vsevolod.rodionov@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "jscs": "^2.7.0",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "cypher-talker": "^0.4.5",
    "debug": "^2.2.0",
    "neo4j": "^2.0.0-RC2",
    "uuid": "^2.0.1"
  }
}
